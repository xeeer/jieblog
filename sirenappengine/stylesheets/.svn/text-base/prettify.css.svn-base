/* Pretty printing styles. Used with prettify.js. */

.str { color: #080; }
.kwd { color: #336699; }
.com { color: #800; }
.typ { color: #993300; }
.lit { color: #066; }
.pun { color: #996699; }
.pln { color: #000; }
.tag { color: #008; }
.atn { color: #606; }
.atv { color: #080; }
.dec { color: #336666; }
pre.prettyprint { padding: 2px; }

@media print {
  .str { color: #060; }
  .kwd { color: #006; font-weight: bold; }
  .com { color: #600; font-style: italic; }
  .typ { color: #404; font-weight: bold; }
  .lit { color: #044; }
  .pun { color: #440; }
  .pln { color: #000; }
  .tag { color: #006; font-weight: bold; }
  .atn { color: #404; }
  .atv { color: #060; }
}
