	padding-left:20px;
	height:18px;
}
#site-link a:hover{
	background: url(/icons/33.png) no-repeat;
}


#right-bar a:hover{
	text-decoration: underline;
}

#right-bar ul {
	display:inline;
	margin: 0px;
	padding:0px;
	
}
#right-bar li {
	line-height:150%;
	font-size: 12px;
	text-align:left;	
	list-style-type:none;	
}

#site-link li:hover {
	background-color: #b5edbc;
}

.feature-list{
	text-align:left;
	padding-left:20px;
}
.feature-list-title {
	background-color: #899dad;
	text-align: left;
	font-size:11px;
	font-weight: bold;
	color: #fff;
	padding-left:5px;
}

.widget-title {
	background-color: #80C65A;
	text-align: left;
