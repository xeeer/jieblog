<div id="feature-list-all">
	<div class="feature-list-title">Feature List</div>
	<div class="feature-lists">
	{% for link in featurelink %}
		<div class="feature-list">{% if link.realized %}<img class="icons" src="../../icons/10.png"/>{% else %}<img class="icons" src="../../icons/11.png"/>{% endif %}<span class="blankspace"/><span class="feature-span">{{link.title}}</span><span class="feature-span">{{link.post_on|date:"Y-m-d"}}</span>{% if show_edit%}<span class="feature-span"><a href="../../editfeature/{{link.get_feature_url}}">edit</a></span>{% endif %}</div>
	{% endfor %}
	</div>
</div>